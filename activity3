public class Activity3{
   public static void main(String[] args){
      double[] someNums = {5.0, 3.0, 1.0, 2.5};
      int a = 1;
      int b;
      double temp;
     
      while(a<someNums.length)
      {
         temp = someNums[a];
         b = a - 1;
         while(b >= 0 && someNums[b] > temp)
         {
            someNums[b + 1] = someNums[b];
            --b;
         }//end of nested while
         someNums[b + 1] = temp;
         ++a;
      }//end of while
   }
}
