public class Student{
    //variables
    private int idNumber;
    private String last;
    private String first;
    private String course;
    private double grade;

    //constructor
    public Student (int idNumber, String last, String first, String course, double grade){
        this.idNumber=idNumber;
        this.last=last;
        this.first=first;
        this.course=course;
        this.grade=grade;
    }

    //getters
    public int getidNumber() {
        return idNumber;
    }
    public String getLast() {
        return last;
    }
    public String getFirst() {
        return first;
    }
    public String getCourse() {
        return course;
    }
    public double getGrade() {
        return grade;
    }


}
